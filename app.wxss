/**app.wxss**/
@import './styles/iconfont.wxss';
.container-100 {
  height: 100vh;
}
.bod-bottom{
  border-bottom: 0.5rpx solid #e5e5e5;
}
.bod-right{
  border-right: 0.5rpx solid #e5e5e5;
}
.bod-top{
  border-top: 0.5rpx solid #e5e5e5;
}
.bod-left{
  border-left: 0.5rpx solid #e5e5e5;
}
.ie-box{
  box-sizing: border-box;
}
.flex-row{
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.flex-column{
  display: flex;
  align-items: flex-start;
  justify-content: center;
  flex-direction: column;
}
.shrink-0{
  flex-shrink: 0;
}
.banner-img-20{
  width: 100%;
  height: 20vh;
}
.banner-img-30{
  width: 100%;
  height: 30vh;
}
.banner-img-40{
  width: 100%;
  height: 40vh;
}
.banner-img-50{
  width: 100%;
  height: 50vh;
}
.label-deep-dark{
  font-size: 30rpx;
  font-weight: 900;
  padding: 20rpx;
}
.padding-20{
  padding: 20rpx;
}
.active-box:active{
  background: #f8f8f8;
}
.btn-submit{
  width: auto !important;
  margin: 0 !important;
  padding: 10rpx 20rpx;
  font-weight: normal;
  border-radius: 50rpx;
  background: #409eff;
  color: #fff;
  border: 2rpx solid transparent;
}
.btn-submit:active{
  background: transparent;
  color: #409eff;
  border: 2rpx solid;
}
.image-small{
  width: 80rpx;
  height: 80rpx;
}
.image-large{
  width: 80rpx;
  height: 80rpx;
}
.image-mini{
  width: 80rpx;
  height: 80rpx;
}
.image-radius{
  border-radius: 50%;
}
.image-radius-10{
  border-radius: 10rpx;
}
.text-40{
  font-size: 40rpx;
}
.text-30{
  font-size: 30rpx;
}
.text-25{
  font-size: 25rpx;
}